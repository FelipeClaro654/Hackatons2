$(function() {
  $("#adicionar_integ_form").click(function () {
    var id =  $(this).data("id");
      $.ajax({
        url: window.location.origin + '/teams/novo_integrante',
        data: {
          id: id
        }
      })
      .done(function(result) {
        $("#include_form").html(result);
      });
  });
});
