<table class="table">
  <thead>
    <tr>
      <th>Nome</th>
      <th>CPF</th>
      <th>E-mail</th>
      <th>Observações</th>
    </tr>
  </thead>
  <tbody>
    <% @team.integrantes.each do |p| %>
      <tr>
        <td><%= p.nome %></td>
        <td><%= p.cpf %></td>
        <td><%= p.email %></td>
        <td><%= p.obs %></td>
      </tr>
    <% end %>
  </tbody>
</table>
