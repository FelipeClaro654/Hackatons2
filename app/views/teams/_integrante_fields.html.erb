
<div class="sub_integrantes nested-fields">
  <div class="row form-integrantes">
    <div class="col-md-4">
      <hr>
      <div class="field form-group">
        <%= f.label :nome, :class => "control-label" %><br>
        <%= f.text_field :nome, :required => true, :class => "form-control" %>
      </div>
      <div class="field form-group">
        <%= f.label :cpf %>
        <%= f.text_field :cpf, :required => true, :class => "cpf-mask form-control" %>
      </div>
      <div class="field form-group">
        <%= f.label :email %>
        <%= f.text_field :email, :required => true, :class => "form-control" %>
      </div>
      <div class="field form-group">
        <%= f.label :obs %>
        <%= f.text_area :obs, :class => "form-control" %>
      </div>
      <%= link_to_remove_association "remove integrante",
      f, {wrapper_class: "sub_integrantes", :class => "btn btn-danger pull-right"}
      %>
    </div>
  </div>
</div>
